{
	"name": "Orders",
	"properties": {
		"activities": [
			{
				"name": "Sales Table",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SnowflakeV2Source",
						"exportSettings": {
							"type": "SnowflakeExportCopyCommand"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "O_ORDERKEY",
									"type": "Decimal"
								},
								"sink": {
									"name": "ORDERKEY"
								}
							},
							{
								"source": {
									"name": "O_CUSTKEY",
									"type": "Decimal"
								},
								"sink": {
									"name": "CUSTKEY"
								}
							},
							{
								"source": {
									"name": "O_ORDERSTATUS",
									"type": "String"
								},
								"sink": {
									"name": "ORDERSTATUS"
								}
							},
							{
								"source": {
									"name": "O_TOTALPRICE",
									"type": "Decimal"
								},
								"sink": {
									"name": "TOTALPRICE"
								}
							},
							{
								"source": {
									"name": "O_ORDERDATE",
									"type": "DateTime"
								},
								"sink": {
									"name": "ORDERDATE"
								}
							},
							{
								"source": {
									"name": "O_ORDERPRIORITY",
									"type": "String"
								},
								"sink": {
									"name": "ORDERPRIORITY"
								}
							},
							{
								"source": {
									"name": "O_CLERK",
									"type": "String"
								},
								"sink": {
									"name": "CLERK"
								}
							},
							{
								"source": {
									"name": "O_SHIPPRIORITY",
									"type": "Decimal"
								},
								"sink": {
									"name": "SHIPPRIORITY"
								}
							},
							{
								"source": {
									"name": "O_COMMENT",
									"type": "String"
								},
								"sink": {
									"name": "COMMENT"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SNOWFLAKE_DB",
						"type": "DatasetReference",
						"parameters": {
							"database": {
								"value": "@pipeline().parameters.database",
								"type": "Expression"
							},
							"warehouse": {
								"value": "@pipeline().parameters.warehouse",
								"type": "Expression"
							},
							"role": {
								"value": "@pipeline().parameters.role",
								"type": "Expression"
							},
							"schema": {
								"value": "@pipeline().parameters.schema",
								"type": "Expression"
							},
							"table_name": {
								"value": "@pipeline().parameters.table",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "BLOB_PARQUET",
						"type": "DatasetReference",
						"parameters": {
							"container": {
								"value": "@pipeline().parameters.blob_container",
								"type": "Expression"
							},
							"folder_path": {
								"value": "@pipeline().parameters.order_folder_path",
								"type": "Expression"
							},
							"file_name": {
								"value": "@concat('orders_',utcnow(),'.parquet')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"database": {
				"type": "string"
			},
			"role": {
				"type": "string"
			},
			"warehouse": {
				"type": "string"
			},
			"schema": {
				"type": "string"
			},
			"table": {
				"type": "string"
			},
			"blob_container": {
				"type": "string"
			},
			"order_folder_path": {
				"type": "string"
			}
		},
		"annotations": []
	}
}