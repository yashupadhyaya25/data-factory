{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Sales Table",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SnowflakeV2Source",
						"exportSettings": {
							"type": "SnowflakeExportCopyCommand"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "dev_db",
						"type": "DatasetReference",
						"parameters": {
							"database": {
								"value": "@pipeline().parameters.database",
								"type": "Expression"
							},
							"warehouse": {
								"value": "@pipeline().parameters.warehouse",
								"type": "Expression"
							},
							"role": {
								"value": "@pipeline().parameters.role",
								"type": "Expression"
							},
							"schema": {
								"value": "@pipeline().parameters.schema",
								"type": "Expression"
							},
							"table_name": {
								"value": "@pipeline().parameters.table",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "BLOB_PARQUET",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"database": {
				"type": "string"
			},
			"role": {
				"type": "string"
			},
			"warehouse": {
				"type": "string"
			},
			"schema": {
				"type": "string"
			},
			"table": {
				"type": "string"
			}
		},
		"annotations": []
	}
}